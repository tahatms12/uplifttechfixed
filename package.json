{
  "name": "uplift-technologies-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npm run copy-fonts && vite build",
    "copy-fonts": "node copyFonts.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "analyze": "vite build --mode analyze",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "vitest run --config vitest.config.a11y.ts",
    "lhci:ci": "lhci --config=./lighthouserc.json",
    "optimize:images": "sharp-cli --input \"public/img/**/*.{jpg,png}\" --output \"public/img/optimized\" --format webp,avif --resize 320,640,1024"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@fontsource/poppins": "^5.2.6",
    "@headlessui/react": "^1.7.17",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.22.0",
    "sharp": "^0.33.2",
    "three": "^0.162.0"
  },
  "devDependencies": {
    "netlify-plugin-vite": "^0.2.0",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/three": "^0.162.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "copyfiles": "^2.4.1",
    "cssnano": "^6.0.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.9.0",
    "http-server": "^14.1.1",
    "jsdom": "^24.0.0",
    "lightningcss": "^1.24.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.12.0",
    "sharp-cli": "^5.2.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.27.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.0.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^3.2.4"
  }
}